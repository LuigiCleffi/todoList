<%- include('header'); -%>
    <div class="box" id="heading">
        <h1>
            <%=listTitle%>
        </h1>
    </div>

    <div class="box">
        <% newListItems.map(item=> {%>

            <div class="item">
                <input type="checkbox" name="" id="">
                <p>
                    <%=item%>
                </p>
            </div>
            <%})%>

                <form action="/" class="item" method="POST">
                    <input autocomplete="off" type="text" name="newTodo" placeholder="New Item" id="">
                    <button type="submit" name="list" value=<%=listTitle%>>+</button>
                </form>

    </div>
    <%- include('footer'); -%>